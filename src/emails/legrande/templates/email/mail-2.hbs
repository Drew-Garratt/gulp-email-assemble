---
subject: Le Grande Fromage
animation: true
---
<table>
  <tr>
    {{> spacer width='100%' height='10px' }}
  </tr>
  <tr>
    <td id="viewonline">
      <p><webversion style="color:#A6A6A6;text-decoration:none">View this email online</webversion></p>
    </td>
  </tr>
  <tr>
    {{> spacer width='100%' height='10px' }}
  </tr>
  <tr>
    <td id="hero" class="section">
      <table>
        <tr>
          <td class="p0">
            {{#> background-image width="100%" width_pixels="600px" height="257px" image="images/hero.png" colour="#FFFFFF"}}
            <td class="p0 background-image">
              <table>
                <tr>
                  {{> spacer width='100%' height='75px' }}
                </tr>
                <tr>
                  <td class="p0 fadeIn0">
                    <a href="https://www.tribeuk.co.uk/legrandefromage/"><img src="images/fromage@2x.png" alt="Le Grande Fromage" width="327" height="110"></a>
                  </td>
                </tr>
                <tr>
                  {{> spacer width='100%' height='75px' }}
                </tr>
              </table>
            </td>
            {{/background-image}}
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td id="main" class="section">
      <table>
        <tr>
          <td class="columns">
            [mso_open]
            <table>
              <tr>
                <td class="outer-col">
                  [mso_close]
                  <div class="outer col"></div>
                  [mso_open]
                </td>
                <td class="content-col">
                  [mso_close]
                  <div class="content col">
                    <table>
                      <tr>
                        <td class="content-block column-single">
                          <table>
                            <tr>
                              <td class="p0 fadeIn">
                                {{{content-2.intro}}}
                              </td>
                            </tr>
                            <tr>
                              {{> spacer width='100%' height='15px' }}
                            </tr>
                            <tr>
                              <td class="p0 fadeIn1">
                                {{{content-2.body-top}}}
                              </td>
                            </tr>
                            <tr>
                              {{> spacer width='100%' height='15px' }}
                            </tr>
                            <tr>
                              <td class="p0 fadeIn2">
                                <div>
                                  [mso_open]
                                  <v:rect xmlns:v="urn:schemas-microsoft-com:vml" xmlns:w="urn:schemas-microsoft-com:office:word" href="mailto:pholmes@tribeuk.co.uk" style="height:30px;v-text-anchor:middle;width:250px;" strokecolor="#9B6C34" fillcolor="#ffffff">
                                    <w:anchorlock/>
                                    <center style="color:#9B6C34;font-family:Arial, sans-serif;font-size:13px;font-weight:bold;">Répondez s’il vous plaît</center>
                                  </v:rect>
                                  [mso_close]
                                  <a href="mailto:pholmes@tribeuk.co.uk" class="button-hover" style="background-color:#ffffff;border:2px solid #9B6C34;color:#9B6C34;display:inline-block;font-family:'Helvetica Neue', 'Helvetica', Helvetica, Arial, sans-serif;font-size:13px;font-weight:bold;line-height:30px;text-align:center;text-decoration:none;width:250px;-webkit-text-size-adjust:none;mso-hide:all;">Répondez s’il vous plaît</a>
                                </div>
                              </td>
                            </tr>
                            <tr>
                              {{> spacer width='100%' height='30px' }}
                            </tr>
                            <tr>
                              <td class="p0 fadeIn3">
                                {{{content-2.body-bottom}}}
                              </td>
                            </tr>
                            <tr>
                              {{> spacer width='100%' height='60px' }}
                            </tr>
                            <tr>
                              <td class="p0 fadeIn4">
                                {{{content-2.footer}}}
                              </td>
                            </tr>
                            <tr>
                              {{> spacer width='100%' height='10px' }}
                            </tr>
                            <tr>
                              <td class="p0"><p><a href="https://www.tribeuk.co.uk/"><img src="images/tribe-logo@2x.png" alt="tribe | Brand Experts" width="164" height="34"></a></p></td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                    </table>
                  </div>
                  [mso_open]
                </td>
                <td class="outer-col">
                  [mso_close]
                  <div class="outer col"></div>
                  [mso_open]
                </td>
              </tr>
            </table>
            [mso_close]
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td id="footer" class="section">
      <p><a href="tel:+441159412184">0115 941 2184</a> | <a href="mailto:hello@tribeuk.co.uk">hello@tribeuk.co.uk</a></p>
      <p>&nbsp;</p>
      <p>&nbsp;</p>
      <p>Not your thing?&nbsp;<unsubscribe style="color:#A6A6A6;text-decoration:none">Just unsubscribe</unsubscribe></p>
    </td>
  </tr>
</table>
